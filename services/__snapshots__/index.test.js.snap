        dataDictionary.marketing.marketingCode = (params['utm_source']?params['utm_source']:\\"\\") + \\"|\\" +
          (params['utm_medium']?params['utm_medium']:\\"\\") + \\"|\\" + (params['utm_term']?params['utm_term']:\\"\\") + \\"|\\" +
          (params['utm_content']?params['utm_content']:\\"\\") + \\"|\\" + (params['utm_name']?params['utm_name']:\\"\\");
      } else if (referrer) {
          for (var i = 0; i < referrers.length; i++) {
