      if (queryString) {
        var paramList = queryString.split('&');

        paramList.forEach(function(param) {
            var key_value = param.split('=');
            params[key_value[0]] = key_value[1]
        }); 
      }
      return params;
    }

    function buildReferrerMarketingCode(url) {
