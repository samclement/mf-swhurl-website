        'koubei.baidu.com','label.baidu.com','prologue.blog.naver.com',
        'section.blog.naver.com','wap.baidu.com', 'xbox.m.baidu.com','xiaomi.baidu.com'
      ];

      if (params.hasOwnProperty('rffrid')) {
        dataDictionary.marketing.marketingCode = params['rffrid'];
      } else if (params.hasOwnProperty('utm_source') || params.hasOwnProperty('utm_medium') ||
        params.hasOwnProperty('utm_term') || params.hasOwnProperty('utm_content') ||
