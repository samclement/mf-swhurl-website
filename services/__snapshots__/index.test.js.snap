    function getParameterByName(name, url_for_parameter) {
      if (!url_for_parameter) {
        url_for_parameter = window.location.href;
      }

      name = name.replace(/[\\\\[\\\\]]/g, \\"\\\\$&\\");
      var regex = new RegExp(\\"[?&]\\" + name + \\"(=([^&#]*)|&|#|$)\\");
      var results = regex.exec(url_for_parameter);

      if (!results) {
        return null;
      }

      if (!results[2]) {
        return '';
      }

      return decodeURIComponent(results[2].replace(/\\\\+/g, \\" \\"));
    }

