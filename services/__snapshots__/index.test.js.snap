            <script id=\\"variantSizeOptionsTemplate\\" type=\\"text/x-handlebars-template\\">
                <select data-mf-select id=\\"{{product.code}}-variants\\">
                {{#if_ne product.sizeTaxonomyName \\"\\"}}
                    <option value=\\"\\" {{if selectedValue == product.code}}selected=\\"selected\\"{{/if}}>
                        {{product.sizeTaxonomyName}}
                    </option>
                {{/if_ne}}
                {{#each product.variantOptions}}
                    <option value=\\"{{code}}\\" data-size-code=\\"{{sizeData.code}}\\"
                        data-base-size-code=\\"{{sizeData.baseCode}}\\"
                        data-stock=\\"{{stock.stockLevelStatus.code}}\\"
                        data-comingsoon=\\"{{comingSoon}}\\"
                        data-url=\\"{{url}}\\" data-size-value=\\"{{sizeData.value}}\\" data-base-product=\\"{{product.code}}\\"
                        {{#if_eq stock.stockLevelStatus.code \\"outOfStock\\"}}
                            {{#if comingSoon}}
                            data-slug-value=\\"{{comingSoonLabel}}\\"
                            {{else}}
                            data-slug-value=\\"{{soldOutLabel}}\\"
                            {{/if}}
                        {{else}}
                            {{#if_eq stock.stockLevelStatus.code \\"lowStock\\"}}
                            data-slug-value=\\"{{lowStockLabel}}\\"
                            {{else}}
                            data-slug-value=\\"\\"
                            {{/if_eq}}
                        {{/if_eq}}
                        data-stockqty=\\"{{stock.stockLevel}}\\" {{#if (selectedValue == code || product.isOneSize)}}selected=\\"selected\\"{{/if}}>
                        {{sizeData.value}}
                        <!-- Not sure about the followinbg two conditions -->
                        {{#if_eq selectedValue code}}
                        true
                        {{/if_eq}}
                        {{#if product.isOneSize}}
                        true
                        {{/if}}

                        {{#if_eq stock.stockLevelStatus.code \\"outOfStock\\"}}
                            {{#if comingSoon}}
                            - {{comingSoonLabel}}
                            {{else}}
                            - {{soldOutLabel}}
                            {{/if}}
                        {{/if_eq}}
                        {{#if_eq stock.stockLevelStatus.code \\"lowStock\\"}}
                        - {{lowStockLabel}}
                        {{/if_eq}}
                    </option>
                    {{/each}}
                </select>
            </script>

