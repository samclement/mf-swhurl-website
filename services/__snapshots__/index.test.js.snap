        'matches fashion',
        'matchesfashion',
        '(not provided)',
        'ë§¤ì¹ì¤í¨ì',
        'ë§¤ì¹ì¤í¨ìë·ì»´',
        'ë§¤ì¹ì¤'
      ];

      var marketingCode = 'seo.' + url.split('/')[0] + '.' + queryParam +'.';

      if (queryParam === 'unknown' && url.match(/^www\\\\.matchesfashion\\\\.com(\\\\/|\\\\/au\\\\/|\\\\/intl\\\\/|\\\\/us\\\\/|\\\\/fr\\\\/|\\\\/en-fr\\\\/|\\\\/kr\\\\/|\\\\/en-kr\\\\/)$|^www\\\\.matchesfashion\\\\.com\\\\/(mens|womens)$|^www\\\\.matchesfashion\\\\.com\\\\/(au|intl|us|fr|en-fr|kr|en-kr)\\\\/(mens|womens)$|^www\\\\.matchesfashion\\\\.com\\\\/contact-us$|^www\\\\.matchesfashion\\\\.com\\\\/(au|intl|us|fr|en-fr|kr|en-kr)\\\\/contact-us$|^www\\\\.matchesfashion\\\\.com\\\\/careers$|^www\\\\.matchesfashion\\\\.com\\\\/(au|intl|us|fr|en-fr|kr|en-kr)\\\\/careers$|^www\\\\.matchesfashion\\\\.com\\\\/stores\\\\/(marylebone|notting-hill|richmond|wimbledon)$|^www\\\\.matchesfashion\\\\.com\\\\/(au|intl|us|fr|en-fr|kr|en-kr)\\\\/stores\\\\/(marylebone|notting-hill|richmond|wimbledon)$/g)
        || brand.indexOf(queryParam) > -1) {
          marketingCode += 'brand';
      } else {
        marketingCode += 'generic';
      }

      return marketingCode;
    }

    function manageMarketingCode(referrer, url) {
      var params = getParameterMapFromURL(url);
      var referrers = [
        'google','naver','bing','yahoo','baidu','yandex','m.blog.naver.com',
        'daum','cafe.naver.com', 'm.cafe.naver.com','blog.naver.com','so.com',
