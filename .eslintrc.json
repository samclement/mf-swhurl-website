{
  "extends": [ 
    "standard", 
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "styled-components-config"
  ],
  "rules": {
    "react/display-name": 0,
    "react/prop-types": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "browser": true
  }
}
