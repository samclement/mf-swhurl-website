{
  "name": "with-styled-components",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.1.5",
    "ignore-loader": "^0.1.2",
    "next": "4.2.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "styled-components": "^2.1.0"
  },
  "license": "ISC",
  "devDependencies": {
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "jest": "^21.2.1",
    "prettier": "^1.9.1",
    "raf": "^3.4.0"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "./.next",
      "./node_modules/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "blocks/**/*.js",
      "elements/**/*.js",
      "pages/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "./coverage/",
      "./node_modules/",
      "./.next",
      "./pages/_document.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
