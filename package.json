{
  "name": "swhurl-website",
  "version": "1.1.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_ENV=test jest",
    "test:e2e": "wdio",
    "lint": "eslint .",
    "precommit": "precise-commits"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samclement/swhurl-website.git"
  },
  "dependencies": {
    "@risingstack/opentracing-auto": "^1.5.2",
    "babel-plugin-styled-components": "^1.8.0",
    "express": "^4.16.2",
    "express-prom-bundle": "^4.2.1",
    "ignore-loader": "^0.1.2",
    "jaeger-client": "^3.10.0",
    "next": "7.0.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "styled-components": "^3.4.9",
    "yarn": "^1.10.1"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "chai": "^4.1.2",
    "chromedriver": "^2.34.1",
    "enzyme": "^3.4.3",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-styled-components-config": "^0.0.2",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.1",
    "npm-run-all": "^4.1.2",
    "precise-commits": "^1.0.2",
    "prettier": "^1.9.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "wdio-chromedriver-service": "^0.1.2",
    "wdio-mocha-framework": "^0.6.3",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^4.9.11"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "./.next",
      "./node_modules/",
      "./test"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./test/coverage",
    "collectCoverageFrom": [
      "components/**/*.js",
      "pages/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "./coverage/",
      "./node_modules/",
      "./.next",
      "./pages/_document.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
