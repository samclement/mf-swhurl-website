{
  "name": "mf-swhurl-website",
  "version": "1.1.0",
  "scripts": {
    "dev": "node ./src/server.js",
    "build": "next build ./src",
    "start": "NODE_ENV=production node ./src/server.js",
    "test": "npm run lint && npm run test:unit",
    "test:ci": "npm run lint && npm run test:jest:ci && npm run test:e2e && npm run test:perf && npm run test:ci:send",
    "test:ci:send": "./send-test-output.sh",
    "test:unit": "NODE_ENV=test jest",
    "test:jest": "jest --watch",
    "test:jest:ci": "JEST_JUNIT_OUTPUT=test/output/junit/js-test-results.xml jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:e2e": "wdio",
    "test:perf": "lighthouse http://website:3000 --quiet --output=json html --config-path=./lighthouse.config.json --chrome-flags=\"--headless --no-sandbox --disable-gpu\" --output-path=./test/output/lighthouse/lighthouse && node ./lighthouse-report-parser.js",
    "lint": "eslint .",
    "precommit": "precise-commits --whitelist=\"./package-lock.json\"",
    "fmt": "prettier --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samclement/mf-swhurl-website.git"
  },
  "dependencies": {
    "@babel/core": "7.6.0",
    "axios": "0.19.2",
    "axios-cache-adapter": "2.5.0",
    "body-scroll-lock": "2.6.2",
    "cheeseburger-menu": "0.0.13",
    "compression": "1.7.4",
    "express": "4.17.1",
    "express-prom-bundle": "5.1.5",
    "ignore-loader": "0.1.2",
    "next": "9.2.1",
    "prom-client": "11.4.0",
    "react": "16.10.0",
    "react-dom": "16.10.0",
    "react-swipe": "^6.0.4",
    "styled-components": "4.2.1",
    "swipe-js-iso": "2.1.5"
  },
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^9.1.4",
    "@wdio/allure-reporter": "^5.4.16",
    "@wdio/cli": "^5.4.17",
    "@wdio/junit-reporter": "^5.9.1",
    "@wdio/local-runner": "^5.4.17",
    "@wdio/mocha-framework": "^5.9.0",
    "@wdio/spec-reporter": "^5.4.15",
    "@wdio/sync": "^5.4.14",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.1.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-styled-components": "1.10.1",
    "chai": "4.2.0",
    "chromedriver": "79.0.0",
    "eslint": "6.4.0",
    "eslint-config-standard": "13.0.1",
    "eslint-config-standard-react": "9.1.0",
    "eslint-plugin-import": "2.19.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-styled-components-config": "0.0.2",
    "husky": "3.0.6",
    "jest": "^24.1.0",
    "jest-junit": "^9.0.0",
    "jest-styled-components": "6.3.4",
    "lighthouse": "^5.6.0",
    "npm-run-all": "4.1.5",
    "precise-commits": "1.0.2",
    "prettier": "1.19.0",
    "raf": "3.4.1",
    "snapshot-diff": "^0.6.0",
    "table": "5.4.0",
    "wdio-chromedriver-service": "^5.0.0",
    "webdriverio": "^5.4.14"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./.next",
      "./node_modules/",
      "./test"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./test/output/jest",
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/pages/**/*.js",
      "src/services/**/*.js",
      "src/lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "./test/output/coverage/",
      "./node_modules/",
      "./.next",
      "./pages/_document.js",
      "./pages/_app.js"
    ],
    "coverageReporters": [
      "json",
      "lcovonly",
      "text",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
